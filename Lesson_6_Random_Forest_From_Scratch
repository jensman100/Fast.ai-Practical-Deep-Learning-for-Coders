    {
      "cell_type": "markdown",
      "source": [
        "Validation and future prediction"
      ],
      "metadata": {
        "id": "MaHaig4xYIcl"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def create_pred(tree, row):\n",
        "  root = tree\n",
        "  while root.prediction == None:\n",
        "    split_column = root.column\n",
        "    split_value = root.split_value\n",
        "    if row[split_column] < split_value:\n",
        "      root = root.left\n",
        "    else:\n",
        "      root = root.right\n",
        "  return root.prediction\n",
        "\n",
        "def test_pred(tree, row, print_=True):\n",
        "  pred = create_pred(tree, row)\n",
        "  actual = row['Back']\n",
        "  if print_:\n",
        "    print(f'Prediction: {pred}, Actual: {actual}')\n",
        "  return pred == actual\n",
        "\n",
        "def find_accuracy(tree, df, print_=False):\n",
        "  correct = 0\n",
        "  for _, row in df.iterrows():\n",
        "    if test_pred(tree, row, print_):\n",
        "      correct += 1\n",
        "  print(f'Accuracy: {correct / len(df)*100}%')\n",
        "  return correct / len(df)"
      ],
      "metadata": {
        "id": "5IPVbBsfVioR"
      },
      "execution_count": 150,
      "outputs": []
    },
    {
