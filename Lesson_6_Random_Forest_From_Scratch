    {
      "cell_type": "code",
      "source": [
        "import numpy as np"
      ],
      "metadata": {
        "id": "FWX5Am4Eoykz"
      },
      "execution_count": 132,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def split_dataframe(df, column, split_value):\n",
        "    left_df = df[df[column] < split_value]\n",
        "    right_df = df[df[column] >= split_value]\n",
        "    return left_df, right_df\n",
        "\n",
        "def percentile_split(df, column):\n",
        "    best_split = None\n",
        "    best_impurity = 1.0\n",
        "\n",
        "    # Create percentiles from 0 to 1 in steps of 0.1\n",
        "    percentiles = np.arange(0, 1.1, 0.1)\n",
        "\n",
        "    col_min = df[column].min()\n",
        "    col_max = df[column].max()\n",
        "    col_range = col_max - col_min\n",
        "\n",
        "    for p in percentiles:\n",
        "        split = col_min + col_range * p\n",
        "        left_df, right_df = split_dataframe(df, column, split)\n",
        "\n",
        "        # Skip if either side is empty\n",
        "        if len(left_df) == 0 or len(right_df) == 0:\n",
        "            continue\n",
        "\n",
        "        left_impurity = find_impurity(left_df)[1]\n",
        "        right_impurity = find_impurity(right_df)[1]\n",
        "\n",
        "        # Weighted impurity\n",
        "        total_impurity = (len(left_df)/len(df)) * left_impurity + (len(right_df)/len(df)) * right_impurity\n",
        "\n",
        "        if total_impurity < best_impurity:\n",
        "            best_split = split\n",
        "            best_impurity = total_impurity\n",
        "\n",
        "    return best_split, best_impurity\n",
        "\n",
        "percentile_split(using_df, 'tall(m)')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "iQD__aoekafK",
        "outputId": "537a58ba-e8fa-4bd3-9def-0d61922ddcb6"
      },
      "execution_count": 133,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "(np.float64(1.89), np.float64(0.3535353535353536))"
            ]
          },
          "metadata": {},
          "execution_count": 133
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def find_best_split(df):\n",
        "\n",
        "  columns = df.columns.drop('Back')\n",
        "  best_split = None\n",
        "  best_impurity = 1\n",
        "  best_column = None\n",
        "\n",
        "  for col in columns:\n",
        "    split, impurity = percentile_split(df, col)\n",
        "    if impurity < best_impurity:\n",
        "      best_split = split\n",
        "      best_impurity = impurity\n",
        "      best_column = col\n",
        "\n",
        "  return best_split, best_impurity, best_column\n",
        "\n",
        "find_best_split(using_df)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "koauyljHpVQA",
        "outputId": "eb692877-34cb-449d-cb15-897e4bb9117d"
      },
      "execution_count": 134,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "(np.float64(106.0), np.float64(0.1313131313131313), 'weight')"
            ]
          },
          "metadata": {},
          "execution_count": 134
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def calculate_accuracy(df, split):\n",
        "  left_df, right_df = split_dataframe(df, split[2], split[0])\n",
        "  left_correct = find_accuracy(left_df) * len(left_df)\n",
        "  right_correct = find_accuracy(right_df) * len(right_df)\n",
        "  accuracy = (left_correct + right_correct) / len(df)\n",
        "  print(f'Splitting {split[2]} at {split[0]}...')\n",
        "  print(f'Gives the accuracty of {accuracy*100}%')\n",
        "  return (left_correct + right_correct) / len(df)\n",
        "\n",
        "split = find_best_split(train_df)\n",
        "calculate_accuracy(verif_df, split)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "SDYmNAiRpoTq",
        "outputId": "676af47c-fcd4-459b-9ab4-85fceda1bcf9"
      },
